pipeline {
    agent any

    environment {
        
        SONARQUBE_URL = 'http://192.168.62.25:9000'
        PROJECT_KEY = 'app-de-test'
        SONAR_NODEJS_PATH = '/usr/bin/node'
    }

stages {
        stage('Checkout') {
            
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/hass2001/app_test.git']]])
        
            }
        }


          stage('SonarQube Analysis') {
    steps {
        script {
            withSonarQubeEnv('SonarQube') {
                sh '''
                export PATH=$PATH:/opt/sonar-scanner/bin
                sonar-scanner -Dsonar.projectKey=app-de-test -Dsonar.sources=. -Dsonar.host.url=http://192.168.62.25:9000 -Dsonar.login=squ_2f2beafc275c5b5f0f54558122885db5cf386d66
                '''
            }
        }
    }
}

      }  

}
